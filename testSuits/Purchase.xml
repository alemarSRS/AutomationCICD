<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="All Test Suite">
    <groups>
        <run>
            <include name="PurchaseTest"/>
        </run>
    </groups>
    <test thread-count="5" name="Submit Order Test">
        <classes>
            <class name="alemar.TestCases.SubmitOrderTest"/>
        </classes>
    </test>

    <test thread-count="5" name="Error validation Test">
        <classes>
            <class name="alemar.TestCases.ErrorValidationsTest"/>
        </classes>
    </test>
</suite>